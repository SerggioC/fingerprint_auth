<?xml version="1.0" encoding="utf-8"?>
<resources>
  <attr name="ridgeColorOn" format="reference|color"/>
  <attr name="ridgeColorOff" format="reference|color"/>
  <attr name="errorColor" format="reference|color"/>
  <attr name="successColor" format="reference|color"/>

  <declare-styleable name="FingerPrintView">
    <!-- Keep in sync with State enum. -->
    <attr name="state" format="enum">
      <enum name="on" value="0"/>
      <enum name="off" value="1"/>
      <enum name="error" value="2"/>
      <enum name="success" value="3"/>
    </attr>
  </declare-styleable>
</resources>
